{
  "compilerOptions": {
    "outDir": "./",
    "module": "es6",
    "target": "es6",
    "lib": [
      "es2017"
    ],
    "types": [
      "@citizenfx/client",
      "@citizenfx/server",
      "@types/node"
    ],
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "diagnostics": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [],
}